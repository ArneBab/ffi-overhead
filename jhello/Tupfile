include_rules

# =====================================

CFLAGS += -I/usr/lib/jvm/java-7-oracle/include -I/usr/lib/jvm/java-7-oracle/include/linux

N1 = newplus
DIR1 = $(PROJECT_DIR)/$(N1)

: foreach *.c |> !cc |>

# ===================================== shared lib containing all the deps

: *.o |> !ar |>
: *.a $(PROJECT_DIR)/$(N1)/_.a |> !shared |>

#: *.a |> !shared |>
